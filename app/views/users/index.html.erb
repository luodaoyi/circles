<h1>用户列表</h1>


<div class="row">
  <div class="col-sm-12">
    <table class="table">
      <tr>
        <th>ID</th>
        <th>用户名</th>
        <th></th>
      </tr>

      <% @users.each do |user| %>

      <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
        <td></td>
      </tr>
      <% end %>

      <%= will_paginate @users %>

    </table>
  </div>
</div>